import turtle

bob = turtle.Turtle()
bob.color("red","black")
bob.begin_fill()

bob.forward(200)
bob.left(135)
bob.forward(200)
bob.forward(200)
bob.left(135)
bob.forward(200)
bob.forward(200)
bob.left(135)
bob.forward(200)
bob.forward(200)
bob.left(135)
bob.forward(200)
bob.forward(200)
bob.left(135)
bob.forward(200)
bob.forward(200)
